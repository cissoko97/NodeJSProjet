<% include header %>
<div class="ui comments">
    <h3 class="ui dividing header">Flowers</h3>
    <% for (flower of flowers){%>
    <div class="ui three column grid">

        <div class="ui card column">
            <div class="ui segment">

            </div>
            <div class="image">
                <img src="data:image/jpeg;base64,">
            </div>
            <div class="content">
                <a class="header"><%= flower.name %></a>
                <div class="description">
                    <%= flower.description %>
                </div>
            </div>
            <div class="extra content">
                <a>
                    <i class="user icon"></i>
                    <%= flower.race %>
                </a>
                <a>
                    <i class="dollar icon"></i>
                    <%= flower.price%>
                </a>
                <a>
                    <form method="POST" action="/flowers/<%= flower.id %>?_method=DELETE" enctype="application/x-www-form-urlencoded">
                        <button type="submit" class="ui icon button">
                            <i class="trash icon"></i>
                        </button>
                    </form>
                </a>
            </div>

        </div>
    </div>
    <%}%>
</div>
<% include footer %>